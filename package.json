{
  "name": "vue-awesome-template",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "commit": "git-cz",
    "release": "standard-version",
    "release:patch": "npm run release -r patch",
    "release:minor": "npm run release -r minor",
    "release:major": "npm run release -r major",
    "release:first": "npm run release -- --first-release",
    "release:publish": "git push --follow-tags origin master",
    "contributor:init": "all-contributors init",
    "contributor:add": "all-contributors add",
    "contributor:generate": "all-contributors generate"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "better-scroll": "^1.13.4",
    "element-ui": "^2.8.2",
    "velocity-animate": "^1.5.2",
    "vue-lazyload": "^1.2.6",
    "core-js": "^3.6.5",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-e2e-cypress": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-pwa": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "all-contributors-cli": "^6.16.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.1.2",
    "css-vars-ponyfill": "^2.3.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "standard-version": "^8.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^4.1.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "vue-cli-plugin-auto-alias": "~1.2.1",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{json,css,scss,less,sass,md,html,flow,ts,tsd}": [
      "prettier --write",
      "git add"
    ]
  }
}
